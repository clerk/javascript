{
  "name": "@clerk/ui",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "author": {
    "name": "Clerk, Inc.",
    "email": "support@clerk.com",
    "url": "git+https://github.com/clerk/javascript.git"
  },
  "exports": {
    "./primitives/*": {
      "import": "./dist/primitives/*.mjs",
      "require": "./dist/primitives/*.js",
      "types": "./src/primitives/*.d.ts"
    },
    "./components/*": {
      "import": "./dist/components/*.mjs",
      "require": "./dist/components/*.js",
      "types": "./src/components/*.d.ts"
    },
    "./styles.css": "./dist/styles.css"
  },
  "typesVersions": {
    "*": {
      "primitives/*": [
        "dist/primitives/*.d.ts"
      ],
      "components/*": [
        "dist/components/*.d.ts"
      ],
      "styles.css": [
        "./dist/styles.css"
      ]
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "clsx": "^2.1.1"
  },
  "devDependencies": {
    "@clerk/eslint-config-custom": "*",
    "@clerk/tailwindcss-transformer": "*"
  }
}
