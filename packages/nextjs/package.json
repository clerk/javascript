{
  "name": "@clerk/nextjs",
  "version": "4.31.8",
  "description": "Clerk SDK for NextJS",
  "keywords": [
    "clerk",
    "typescript",
    "nextjs",
    "auth",
    "authentication",
    "passwordless",
    "session",
    "jwt"
  ],
  "homepage": "https://clerk.com/",
  "bugs": {
    "url": "https://github.com/clerk/javascript/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clerk/javascript.git",
    "directory": "packages/nextjs"
  },
  "license": "MIT",
  "author": "Clerk",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./server": {
      "types": "./dist/types/server/index.d.ts",
      "import": "./dist/esm/server/index.js",
      "require": "./dist/cjs/server/index.js"
    },
    "./api": {
      "types": "./dist/types/api/index.d.ts",
      "import": "./dist/esm/api/index.js",
      "require": "./dist/cjs/api/index.js"
    },
    "./ssr": {
      "types": "./dist/types/ssr/index.d.ts",
      "import": "./dist/esm/ssr/index.js",
      "require": "./dist/cjs/ssr/index.js"
    },
    "./edge-middlewarefiles": {
      "types": "./dist/types/edge-middleware/index.d.ts",
      "import": "./dist/esm/edge-middleware/index.js",
      "require": "./dist/cjs/edge-middleware/index.js"
    },
    "./app-beta": {
      "types": "./dist/types/app-beta/index.d.ts",
      "import": "./dist/esm/app-beta/index.js",
      "require": "./dist/cjs/app-beta/index.js"
    },
    "./app-beta/client": {
      "types": "./dist/types/app-beta/client/index.d.ts",
      "import": "./dist/esm/app-beta/client/index.js",
      "require": "./dist/cjs/app-beta/client/index.js"
    }
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "server",
    "api",
    "edge-middleware",
    "ssr",
    "app-beta"
  ],
  "scripts": {
    "build": "npm run clean && tsup",
    "build:declarations": "tsc -p tsconfig.declarations.json",
    "clean": "rimraf ./dist",
    "dev": "tsup --watch",
    "dev:publish": "npm run dev -- --env.publish",
    "lint": "eslint src/",
    "lint:attw": "attw --pack .",
    "lint:publint": "publint",
    "publish:local": "npx yalc push --replace --sig",
    "test": "jest",
    "test:cache:clear": "jest --clearCache --useStderr",
    "test:ci": "jest --maxWorkers=70%"
  },
  "dependencies": {
    "@clerk/backend": "0.38.15",
    "@clerk/clerk-react": "4.32.5",
    "@clerk/clerk-sdk-node": "4.13.23",
    "@clerk/shared": "1.4.2",
    "@clerk/types": "3.65.5",
    "tslib": "2.4.1"
  },
  "devDependencies": {
    "@types/node": "^16.11.55",
    "@types/react": "*",
    "@types/react-dom": "*",
    "next": "^13.4.19",
    "node-fetch-native": "1.1.1",
    "typescript": "*"
  },
  "peerDependencies": {
    "next": "^10 || ^13.5.7 || ^14.2.25 || ^15.2.3",
    "react": "^17.0.2 || ^18.0.0-0",
    "react-dom": "^17.0.2 || ^18.0.0-0"
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public"
  }
}
