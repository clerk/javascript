{
  "name": "@clerk/tanstack-start",
  "version": "0.0.1",
  "description": "Clerk SDK for TanStack Start",
  "keywords": [
    "clerk",
    "typescript",
    "tanstack start",
    "auth",
    "authentication",
    "passwordless",
    "session",
    "jwt"
  ],
  "homepage": "https://clerk.com/",
  "bugs": {
    "url": "https://github.com/clerk/javascript/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clerk/javascript.git",
    "directory": "packages/tanstack-start"
  },
  "license": "MIT",
  "author": "Clerk",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./server": {
      "import": {
        "types": "./dist/esm/server/index.d.ts",
        "default": "./dist/esm/server/index.js"
      },
      "require": {
        "types": "./dist/cjs/server/index.d.cts",
        "default": "./dist/cjs/server/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist",
    "server"
  ],
  "scripts": {
    "build": "npm run clean && tsup",
    "build:declarations": "tsc -p tsconfig.declarations.json",
    "clean": "rimraf ./dist",
    "dev": "tsup --watch",
    "dev:publish": "npm run dev -- --env.publish",
    "lint": "eslint src/",
    "lint:attw": "attw --pack .",
    "lint:publint": "publint",
    "publish:local": "npx yalc push --replace --sig"
  },
  "dependencies": {
    "@clerk/backend": "1.2.1",
    "@clerk/clerk-react": "5.2.2",
    "@clerk/shared": "2.2.1",
    "@clerk/types": "4.5.1",
    "@vinxi/server-functions": "^0.3.2",
    "tslib": "2.4.1"
  },
  "devDependencies": {
    "@clerk/eslint-config-custom": "*",
    "@tanstack/react-router": "^1.34.9",
    "@tanstack/start": "^1.34.9",
    "@types/node": "^18.17.0",
    "@types/react": "*",
    "@types/react-dom": "*",
    "esbuild-plugin-file-path-extensions": "^2.1.1",
    "typescript": "*"
  },
  "peerDependencies": {
    "@tanstack/react-router": ">=1.34.9",
    "@tanstack/start": ">=1.34.9",
    "react": ">=18 || >=19.0.0-beta",
    "react-dom": ">=18 || >=19.0.0-beta"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
