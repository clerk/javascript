{
  "name": "@clerk/tanstack-start",
  "version": "0.0.0",
  "description": "Clerk SDK for TanStack Start",
  "keywords": [
    "clerk",
    "typescript",
    "tanstack start",
    "auth",
    "authentication",
    "passwordless",
    "session",
    "jwt"
  ],
  "homepage": "https://clerk.com/",
  "bugs": {
    "url": "https://github.com/clerk/javascript/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clerk/javascript.git",
    "directory": "packages/tanstack-start"
  },
  "license": "MIT",
  "author": "Clerk",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/types/client/index.d.ts",
      "import": "./dist/server/index.js"
    }
  },
  "main": "dist/cjs/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && tsup",
    "build:declarations": "tsc -p tsconfig.declarations.json",
    "clean": "rimraf ./dist",
    "dev": "tsup --watch",
    "dev:publish": "npm run dev -- --env.publish",
    "lint": "eslint src/",
    "lint:attw": "attw --pack .",
    "lint:publint": "publint",
    "publish:local": "npx yalc push --replace --sig",
    "test": "jest",
    "test:cache:clear": "jest --clearCache --useStderr",
    "test:ci": "jest --maxWorkers=70%"
  },
  "dependencies": {
    "@clerk/backend": "1.2.1",
    "@clerk/clerk-react": "5.2.2",
    "@clerk/shared": "2.2.1",
    "@clerk/types": "4.5.1",
    "tslib": "2.4.1"
  },
  "devDependencies": {
    "@clerk/eslint-config-custom": "*",
    "@tanstack/react-router": "^1.34.9",
    "@tanstack/start": "^1.34.9",
    "@types/node": "^18.17.0",
    "@types/react": "*",
    "@types/react-dom": "*",
    "typescript": "*"
  },
  "peerDependencies": {
    "react": ">=18 || >=19.0.0-beta",
    "react-dom": ">=18 || >=19.0.0-beta"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
