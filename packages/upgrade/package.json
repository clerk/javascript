{
  "name": "@clerk/upgrade",
  "version": "1.2.4",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clerk/javascript.git",
    "directory": "packages/upgrade"
  },
  "license": "MIT",
  "type": "module",
  "main": "dist/cli.js",
  "bin": {
    "clerk-upgrade": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm clean && NODE_ENV=production babel --keep-file-extension --out-dir=dist src --copy-files",
    "clean": "rm -rf dist/*",
    "dev": "babel --keep-file-extension --out-dir=dist --watch src --copy-files",
    "format": "node ../../scripts/format-package.mjs",
    "format:check": "node ../../scripts/format-package.mjs --check",
    "lint": "eslint src/",
    "lint:publint": "publint",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "dependencies": {
    "ejs": "3.1.10",
    "execa": "9.4.1",
    "globby": "^14.0.1",
    "gray-matter": "^4.0.3",
    "index-to-position": "^0.1.2",
    "jscodeshift": "^17.0.0",
    "marked": "^11.1.1",
    "meow": "^11.0.0",
    "read-pkg": "^9.0.1",
    "semver-regex": "^4.0.5",
    "temp-dir": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@types/jscodeshift": "^0.12.0"
  },
  "engines": {
    "node": ">=20.9.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
