---
// @ts-expect-error: Virtual module
import { isStaticOutput } from "virtual:@clerk/astro/config";

type Props = {
  isStatic?: boolean
}

const module = isStaticOutput(Astro.props.isStatic) ? await import('./SignedOutCSR.astro') : await import('./SignedOutSSR.astro');
const SignedOutComponent = module.default;
---

<SignedOutComponent>
  <slot />
</SignedOutComponent>
