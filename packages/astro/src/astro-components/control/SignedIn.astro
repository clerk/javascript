---
// @ts-expect-error: Virtual module
import { isStaticOutput } from "virtual:@clerk/astro/config";

type Props = {
  isStatic?: boolean
}

const module = isStaticOutput(Astro.props.isStatic) ? await import('./SignedInCSR.astro') : await import('./SignedInSSR.astro');
const SignedInComponent = module.default;
---

<SignedInComponent>
  <slot />
</SignedInComponent>
