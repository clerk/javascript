{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "deploy": "wrangler publish",
    "dev:remix": "remix watch",
    "dev:miniflare": "cross-env NODE_ENV=development miniflare ./build/index.js --watch",
    "dev": "remix build && run-p \"dev:*\"",
    "start": "cross-env NODE_ENV=production miniflare ./build/index.js",
    "yalc:add": "yalc add @clerk/types && yalc add @clerk/remix && yalc add @clerk/backend"
  },
  "dependencies": {
    "@clerk/backend": "file:.yalc/@clerk/backend",
    "@clerk/remix": "file:.yalc/@clerk/remix",
    "@clerk/types": "file:.yalc/@clerk/types",
    "@remix-run/cloudflare": "^1.7.5",
    "@remix-run/cloudflare-workers": "^1.7.5",
    "@remix-run/react": "^1.7.5",
    "cross-env": "^7.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.18.0",
    "@remix-run/dev": "^1.7.5",
    "@remix-run/eslint-config": "^1.7.5",
    "@types/react": "^17.0.52",
    "@types/react-dom": "^17.0.18",
    "eslint": "^8.27.0",
    "miniflare": "^2.11.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.8.4",
    "wrangler": "^2.2.1"
  },
  "engines": {
    "node": ">=16.13"
  }
}
