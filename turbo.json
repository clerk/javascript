    },
    "//#test:integration:nuxt": {
      "dependsOn": ["@clerk/clerk-js#build", "@clerk/vue#build", "@clerk/nuxt#build"],
      "env": ["CLEANUP", "DEBUG", "E2E_*", "INTEGRATION_INSTANCE_KEYS"],
      "inputs": ["integration/**"],
      "outputLogs": "new-only"
    }
  }
}
