{
  "$schema": "https://turborepo.org/schema.json",
  "remoteCache": {
    "signature": true
  },
  "globalDependencies": [
    ".github/.cache-version",
    "jest.*.ts",
    "package.json",
    "package-lock.json",
    "tsconfig.json",
    "tsconfig.*.json",
    "scripts/subpath-workaround.mjs"
  ],
  "globalEnv": [
    "CLERK_*",
    "GATSBY_CLERK_*",
    "NEXT_PUBLIC_CLERK_*",
    "NODE_ENV",
    "NODE_VERSION",
    "NPM_VERSION",
    "TZ",
    "VERCEL",
    "VITE_CLERK_*",
    "EXPO_PUBLIC_CLERK_*",
    "REACT_APP_CLERK_*"
  ],
  "globalPassThroughEnv": ["AWS_SECRET_KEY", "GITHUB_TOKEN"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": [
        "*.d.ts",
        "bundlewatch.config.json",
        "src/**",
        "tsconfig.json",
        "tsconfig.build.json",
        "tsconfig.declarations.json",
        "tsup.config.ts",
        "subpaths.mjs",
        "!**/**/*.test.*",
        "!**/test/**",
        "!**/tests/**",
        "!**/__tests__/**",
        "!**/__mocks__/**",
        "!**/__snapshots__/**",
        "!CHANGELOG.md",
        "!coverage/**",
        "!dist/**",
        "!examples/**",
        "!node_modules/**"
      ],
      "outputMode": "new-only",
      "outputs": ["*/package.json", "dist/**", "scripts/**"]
    },
    "dev": {
      "dependsOn": [],
      "cache": false
    },
    "dev:current": {
      "cache": false,
      "dependsOn": ["^build", "dev"],
      "persistent": true
    },
    "clean": {
      "dependsOn": [],
      "cache": false
    },
    "test": {
      "dependsOn": ["build"],
      "inputs": [
        "*.d.ts",
        "bundlewatch.config.json",
        "jest.*",
        "src/**",
        "tests/**",
        "tsconfig.json",
        "tsconfig.*.json",
        "tsup.config.ts",
        "!**/__snapshots__/**",
        "!CHANGELOG.md",
        "!coverage/**",
        "!dist/**",
        "!examples/**",
        "!node_modules/**"
      ],
      "outputMode": "new-only",
      "outputs": []
    },
    "test:cache:clear": {
      "cache": false
    },
    "lint": {
      "dependsOn": ["^build"],
      "inputs": [
        "**/*.js",
        "**/*.jsx",
        "**/*.ts",
        "**/*.tsx",
        "**/*.json",
        "**/*.md",
        "**/*.mdx",
        ".github/**",

        "!*/package.json",
        "!**/__snapshots__/**",
        "!CHANGELOG.md",
        "!coverage/**",
        "!dist/**",
        "!examples/**",
        "!node_modules/**"
      ],
      "outputs": []
    },
    "lint:publint": {
      "dependsOn": ["build"],
      "outputs": [],
      "cache": false
    },
    "lint:attw": {
      "dependsOn": [],
      "outputs": [],
      "cache": false
    },
    "bundlewatch": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "format:check": {
      "dependsOn": [],
      "outputs": []
    },
    "//#test:integration:ap-flows": {
      "dependsOn": ["^@clerk/nextjs#build"],
      "env": ["CLEANUP", "DEBUG", "E2E_*", "INTEGRATION_INSTANCE_KEYS"],
      "inputs": ["integration/**"],
      "outputMode": "new-only"
    },
    "//#test:integration:generic": {
      "dependsOn": ["^@clerk/clerk-react#build"],
      "env": ["CLEANUP", "DEBUG", "E2E_*", "INTEGRATION_INSTANCE_KEYS"],
      "inputs": ["integration/**"],
      "outputMode": "new-only"
    },
    "//#test:integration:express": {
      "dependsOn": ["^@clerk/clerk-sdk-node#build", "^@clerk/clerk-react#build"],
      "env": ["CLEANUP", "DEBUG", "E2E_*", "INTEGRATION_INSTANCE_KEYS"],
      "inputs": ["integration/**"],
      "outputMode": "new-only"
    },
    "//#test:integration:nextjs": {
      "dependsOn": ["^@clerk/nextjs#build"],
      "env": ["CLEANUP", "DEBUG", "E2E_*", "INTEGRATION_INSTANCE_KEYS"],
      "inputs": ["integration/**"],
      "outputMode": "new-only"
    },
    "//#test:integration:quickstart": {
      "dependsOn": ["^@clerk/nextjs#build"],
      "env": ["CLEANUP", "DEBUG", "E2E_*", "INTEGRATION_INSTANCE_KEYS"],
      "inputs": ["integration/**"],
      "outputMode": "new-only"
    },
    "//#test:integration:elements": {
      "dependsOn": ["^@clerk/nextjs#build", "^@clerk/elements#build"],
      "env": ["CLEANUP", "DEBUG", "E2E_*", "INTEGRATION_INSTANCE_KEYS"],
      "inputs": ["integration/**"],
      "outputMode": "new-only"
    }
  }
}
