    },
    "//#test:integration:vue": {
      "dependsOn": ["@clerk/clerk-js#build", "@clerk/vue#build"],
      "env": ["CLEANUP", "DEBUG", "E2E_*", "INTEGRATION_INSTANCE_KEYS"],
